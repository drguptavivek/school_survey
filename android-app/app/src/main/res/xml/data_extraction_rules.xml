<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="file" path="shared_prefs/" />
        <exclude domain="database" path="survey_database" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="file" path="shared_prefs/" />
        <exclude domain="database" path="survey_database" />
    </device-transfer>
</data-extraction-rules>